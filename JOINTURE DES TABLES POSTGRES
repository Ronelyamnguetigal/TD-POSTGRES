SELECT vaccin.nom_vaccin,nouveau_nee.nom_bebe,hopital.nom_hopital ,parent.id_parent FROM vaccin 
INNER JOIN nouveau_nee on vaccin.id_vaccin=nouveau_nee.id_nouveau_nee
INNER JOIN hopital on vaccin.id_vaccin =hopital.id_hopital
INNER JOIN parent on vaccin.id_vaccin =parent.id_parent;


